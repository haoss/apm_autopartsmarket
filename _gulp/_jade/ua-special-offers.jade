doctype

html(lang="ru")
  
  -var active = 'Cпециальные предложения' //активный пункт меню, список в header.jade
  -var pageTitle = 'ЛЧ Cпециальные предложения' + ' - Биржа автозапчастей' //title страницы для генерации верстки
  -var headerBlock = 'ЛЧ'  

  include ./_include/_head

  body
    
    // Preloader
    //- include ./_include/_preloader
    
    include ./_include/_user__navigation
  
    #panel
      include ./_include/_header
      
      // Search
