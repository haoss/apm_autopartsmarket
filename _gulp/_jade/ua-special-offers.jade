              .user__search__wrapper.table
                div.table__cell
                  .user__search__body
                    input(type="text" placeholder="Введите номер детали").form-control
                    button(type="submit")
                      i.ion-android-search
                        
                  a(href="#").link-catalog.pull-left Оригинальный каталог
                  a(href="#").link-history.pull-right История запросов
                  
                div.table__cell
                  a(href="#").btn.btn--xsl
